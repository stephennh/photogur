<h1> Edit Picture </h1>

<div class="buttons"><%= link_to "Back", @picture %></div>

<%= form_for @picture do |f| %>
<div class="edit">
  <div>
    <%= f.label :artist %>
    <%= f.text_field :artist %>
  </div>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :url %>
    <%= f.text_field :url %>
  </div>
</div>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>
